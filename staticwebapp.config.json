{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_next/*",
      "/favicon.ico",
      "/manifest.json",
      "/*.png",
      "/*.ico",
      "/*.js",
      "/*.css"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}
